{
  "main":{
    "pattern":[
      "#name_type",
      "\n",
      "Modifiers:",
      "#mods",
      "|",
      "#mods",
      "|",
      "#mods"
    ]
  },
  "#name_type":{
    "bucket":[
      "!castle"
    ],
    "!castle":{
      "pattern":[
        "Name:",
        "!name",
        "\n",
        "Type:",
        "Castle"
      ],
      "!name":{
        "pattern":[
          "!prefix",
          "!suffix"
        ],
        "!prefix":{
          "bucket":[
            "Kaer",
            "Keep",
            "Castle",
            "Fort",
            "High Seat"
          ]
        },
        "!suffix":{
          "bucket":[
            "Hillwatch",
            "Cliffwatch",
            "Lionrock",
            "Bearstone",
            "Blackwatch",
            "Gatewatch",
            "Riverwatch",
            "Morhan",
            "Danspire",
            "Dariol",
            "Rafecliffe"
          ]
        }
      }
    }
  },
  "#mods":{
    "bucket":[
      "dark",
      "bright",
      "damp",
      "musty",
      "lively",
      "rich",
      "spooky",
      "dangerous"
    ]
  }
}

