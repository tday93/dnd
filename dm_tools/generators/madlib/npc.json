{
  "main":{
    "pattern":[
      "Name: ",
      "#name",
      "\n",
      "Disposition: ",
      "#disposition",
      "\n",
      "Appearance: ",
      "#appearance"
    ]
  },
  "#name":{
    "bucket":[
      "#human_name",
      "#dwarf_name"
    ]
  },
  "#human_name":{
    "pattern":[
      "!first",
      " ",
      "!last"
    ],
    "!first":{
      "bucket":[
        "Dave",
        "Amber",
        "Glenn",
        "Mark",
        "John",
        "Olivia",
        "Bobcat",
        "Maria",
        "Theresa"
      ]
    },
    "!last":{
      "bucket":[
        "Johnson",
        "Robertson",
        "Cooper",
        "Smith",
        "Fletcher",
        "Black",
        "Goldthwait",
        "Crowley"
      ]
    }
  },
  "#dwarf_name":{
    "pattern":[
      "!first",
      " ",
      "!last"
    ],
    "!first":{
      "pattern":[
        "!syllable",
        "!syllable",
        "!syllable"
      ],
      "!syllable":{
        "bucket":[
          "Ka",
          "Ra",
          "Ga",
          "Ba",
          "Ke",
          "Re",
          "Ge",
          "Be",
          "Ko",
          "Ro",
          "Go",
          "Bo"
        ]
      }
    },
    "!last":{
      "pattern":[
        "!pre",
        "!suf"
      ],
      "!pre":{
        "bucket":[
          "Iron",
          "Stone",
          "Bronze",
          "Steel",
          "Gold",
          "Hill"
        ]
      },
      "!suf":{
        "bucket":[
          "hammer",
          "arm",
          "fist",
          "foot",
          "pick",
          "forge"
        ]
      }
    }
  },
  "#disposition":{
    "bucket":[
      "friendly",
      "snarky",
      "sly",
      "soober",
      "soft-spoken",
      "boisterous",
      "unamused",
      "kind",
      "worried",
      "quiet",
      "nervous",
      "anxious"
    ]
  },
  "#appearance":{
    "pattern":[
      "!descriptor",
      " | ",
      "!descriptor",
      " | ",
      "!descriptor"
    ],
    "!descriptor":{
      "bucket":[
        "broken nose",
        "splotchy complexion",
        "sharp features",
        "hawkish",
        "handsome",
        "gorgeous",
        "ugly",
        "tall",
        "short",
        "broad shouldered",
        "dark hair",
        "light hair",
        "hunchbacked",
        "slouched",
        "lithe",
        "waifish",
        "jacked",
        "fat",
        "chubby",
        "bowlegged",
        "grizzled",
        "scarred"
      ]
    }
  }
}




